{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "ec2:StopInstances",
        "rds:StopDBInstance",
        "redshift:DeleteCluster",
        "elasticache:DeleteCacheCluster",
        "elasticloadbalancing:DeleteLoadBalancer",
        "elasticbeanstalk:*",
        "lambda:InvokeFunction",
        "s3:ListBucket",
        "s3:DeleteObject"
      ],
      "Resource": "*"
    }
  ]
}
